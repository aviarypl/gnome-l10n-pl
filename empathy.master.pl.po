msgstr "Nie podano powodu"

#: ../libempathy/empathy-tp-file.c:401
msgid "The change in state was requested"
msgstr "Poproszono o zmianę statusu"

#: ../libempathy/empathy-tp-file.c:404
msgid "You canceled the file transfer"
msgstr "Przesyłanie pliku zostało anulowane przez nadawcę"

#: ../libempathy/empathy-tp-file.c:407
msgid "The other participant canceled the file transfer"
msgstr "Przesyłanie pliku zostało anulowane przez odbiorcę"

#: ../libempathy/empathy-tp-file.c:410
msgid "Error while trying to transfer the file"
msgstr "Błąd podczas próby przesłania pliku"

#: ../libempathy/empathy-tp-file.c:413
msgid "The other participant is unable to transfer the file"
msgstr "Drugi uczestnik nie może przesłać pliku"

#: ../libempathy/empathy-tp-file.c:416
msgid "Unknown reason"
msgstr "Nieznany powód"

